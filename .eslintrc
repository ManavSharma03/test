{
        "extends": "airbnb",
        "env": {
            "es6": true,
            "node": true
        },
        "parser": "babel-eslint",
        "rules": {
            "indent": [
                2,
                "tab",
                {
                    "SwitchCase": 1,
                    "VariableDeclarator": 1
                }
            ],
            "no-tabs": 0,
            "no-async-promise-executor": 0,
            "no-underscore-dangle": 0,
            "import/named": 0
        }
}